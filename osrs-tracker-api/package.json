{
    "name": "osrs-tracker-api",
    "version": "0.0.5",
    "author": "Freek Mencke",
    "homepage": "https://twitter.com/FreekMencke",
    "private": true,
    "scripts": {
        "start": "webpack --config webpack.config.js --progress --env.mode=development && start http://localhost:4300 && node dist/osrs-tracker-api.js",
        "start:prod": "webpack --config webpack.config.js --progress --env.mode=production && start http://localhost:4300 && node dist/osrs-tracker-api.js",
        "build:prod": "webpack --config webpack.config.js --progress --env.mode=production",
        "build:ci": "webpack --config webpack.config.js --env.mode=production",
        "visual:dev": "webpack --config webpack.config.js --progress --env.mode=development --env.visualise && start dist/stats.html",
        "visual:prod": "webpack --config webpack.config.js --progress --env.mode=production --env.visualise && start dist/stats.html",
        "lint": "tslint --project ."
    },
    "dependencies": {
        "body-parser": "1.18.3",
        "chalk": "2.4.1",
        "cors": "2.8.5",
        "cron": "1.5.0",
        "express": "4.16.4",
        "helmet": "3.15.0",
        "mysql": "2.16.0",
        "node-fetch": "2.3.0"
    },
    "devDependencies": {
        "@types/body-parser": "1.17.0",
        "@types/cors": "2.8.4",
        "@types/cron": "1.3.0",
        "@types/express": "4.16.0",
        "@types/helmet": "0.0.42",
        "@types/mysql": "2.15.5",
        "@types/node": "10.12.7",
        "@types/node-fetch": "2.1.2",
        "clean-webpack-plugin": "1.0.0",
        "terser-webpack-plugin": "1.1.0",
        "ts-loader": "5.3.0",
        "tslint": "5.11.0",
        "tslint-eslint-rules": "5.4.0",
        "typescript": "3.1.6",
        "webpack": "4.25.1",
        "webpack-cli": "3.1.2",
        "webpack-visualizer-plugin": "0.1.11"
    }
}
